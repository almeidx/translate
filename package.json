		"eslint": "^8.23.1",
		"eslint-config-neon": "^0.1.33",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.8.3",
		"vitest": "^0.23.4"
	},
	"keywords": [
		"translation",
		"translate",
		"google translate",
		"i18n",
		"google"
	],
	"bugs": {
		"url": "https://github.com/almeidx/translate/issues"
	},
	"homepage": "https://github.com/almeidx/translate#readme",
